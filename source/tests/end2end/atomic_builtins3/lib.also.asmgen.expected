test2_atomic_exchange16:
    push %rbp
    mov %rsp, %rbp
    sub $16, %rsp
    mov %rdi, %rax
    movsx %si, %rcx
    movw %cx, -2(%rbp)
    movw %cx, -4(%rbp)
    mov $2, %rcx
    lea -2(%rbp), %rdx
    lea -4(%rbp), %r8
    mov $5, %r9
    mov %rcx, %rdi
    mov %rax, %rsi
    mov %r8, %rcx
    mov %r9, %r8
    call __atomic_exchange@PLT
    movsxw -4(%rbp), %rax
    lea (%rbp), %rsp
    pop %rbp
    ret

