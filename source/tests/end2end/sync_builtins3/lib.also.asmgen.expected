test_sync_bool_compare_and_swap16:
    push %rbp
    mov %rsp, %rbp
    sub $16, %rsp
    mov %rdi, %rax
    movsx %si, %rcx
    movw %cx, -4(%rbp)
    movsx %dx, %rcx
    movw %cx, -2(%rbp)
    mov $2, %rcx
    lea -4(%rbp), %rdx
    lea -2(%rbp), %r8
    mov $5, %r9
    mov $5, %r10
    mov %rcx, %rdi
    mov %rax, %rsi
    mov %r8, %rcx
    mov %r9, %r8
    mov %r10, %r9
    call __atomic_compare_exchange@PLT
    lea (%rbp), %rsp
    pop %rbp
    ret

