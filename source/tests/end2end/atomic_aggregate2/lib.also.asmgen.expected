.att_syntax
.section .note.GNU-stack,"",%progbits

.global load_buffer
.global store_buffer
.extern __kefir_opt_float32_neg
.extern __kefir_opt_float64_neg
.extern __kefirrt_opt_float32_to_uint
.extern __kefirrt_opt_float64_to_uint

.section .text
load_buffer:
    push %rbp
    mov %rsp, %rbp
    sub $80, %rsp
    movq %rdi, -80(%rbp)
    lea -64(%rbp), %r8
    movq %r8, -72(%rbp)
    mov %r8, %rdx
    mov $64, %rdi
    mov $5, %rcx
    call __atomic_load@PLT
    movq -72(%rbp), %r8
    movq -80(%rbp), %rax
    movdqu (%r8), %xmm0
    movdqu %xmm0, (%rax)
    movdqu 16(%r8), %xmm0
    movdqu %xmm0, 16(%rax)
    movdqu 32(%r8), %xmm0
    movdqu %xmm0, 32(%rax)
    movdqu 48(%r8), %xmm0
    movdqu %xmm0, 48(%rax)
    lea (%rbp), %rsp
    pop %rbp
    ret

store_buffer:
    push %rbp
    mov %rsp, %rbp
    sub $64, %rsp
    mov %rsi, %rax
    mov %rdi, %rsi
    mov %rax, %rdx
    mov $64, %rdi
    mov $5, %rcx
    call __atomic_store@PLT
    lea (%rbp), %rsp
    pop %rbp
    ret


