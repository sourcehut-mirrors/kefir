.att_syntax
.section .note.GNU-stack,"",%progbits

.global get_ld1
.global set_ld1
.extern ld1
.extern __kefir_opt_float32_neg
.extern __kefir_opt_float64_neg
.extern __kefirrt_opt_float32_to_uint
.extern __kefirrt_opt_float64_to_uint

.section .text
get_ld1:
    push %rbp
    mov %rsp, %rbp
    sub $32, %rsp
    movq ld1@GOTPCREL(%rip), %rax
    lea -32(%rbp), %rdx
    mov %rax, %rsi
    mov $16, %rdi
    mov $5, %rcx
    call __atomic_load@PLT
    fldt -32(%rbp)
    lea (%rbp), %rsp
    pop %rbp
    ret

set_ld1:
    push %rbp
    mov %rsp, %rbp
    sub $48, %rsp
    lea 16(%rbp), %rax
    movq $0, -24(%rbp)
    fldt (%rax)
    fstpt -32(%rbp)
    fldt -32(%rbp)
    fstpt -48(%rbp)
    movq ld1@GOTPCREL(%rip), %rax
    mov %rax, %rsi
    lea -48(%rbp), %rdx
    mov $16, %rdi
    mov $5, %rcx
    call __atomic_store@PLT
    lea (%rbp), %rsp
    pop %rbp
    ret


